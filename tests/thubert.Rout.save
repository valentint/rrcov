+ 
+         fm <- PcaLocantore(z, k = 2, scale = TRUE)
+         fm@scale
+         apply(z, MARGIN = 2, FUN = mad)
+         scale(z, center = fm@center, scale = fm@scale)
+ 
+         T <- fm@scores
+         P <- fm@loadings
+         E <- scale(z, center = fm@center, scale = fm@scale) - T %*% t(P)
+         d2 <- apply(E^2, MARGIN = 1, FUN = sum)
